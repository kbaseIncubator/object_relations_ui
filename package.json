{
  "name": "object_relations_ui",
  "version": "1.0.0",
  "description": "KBase object relations UI",
  "private": true,
  "scripts": {
    "test": "tsc --project tsconfig.json --noEmit && prettier --check \"src/**/*\"",
    "dev": "parcel src/index.html",
    "build": "(bash -c 'rm -rf $(pwd)/docs/*.{js,css,html,map}') && parcel build --out-dir docs --public-url /object_relations_ui src/index.html",
    "fix": "prettier --write \"src/**/*\""
  },
  "author": "jayrbolton",
  "license": "MIT",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "preact": "^10.4.1"
  }
}
